{
    "name": "presttec\/laravel-airlock",
    "description": "Laravel Airlock provides a featherweight authentication system for SPAs and simple APIs.",
    "keywords": [
        "laravel",
        "airlock",
        "auth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-airlock\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-airlock"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "presttec\/illuminate-contracts": "^6.0|^7.0",
        "presttec\/illuminate-database": "^6.0|^7.0",
        "presttec\/illuminate-support": "^6.0|^7.0"
    },
    "require-dev": {
        "presttec\/mockery": "^1.0",
        "presttec\/testbench": "^4.0|^5.0",
        "presttec\/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Airlock\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Airlock\\Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Airlock\\AirlockServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/airlock": "*"
    },
    "conflicts": {
        "laravel\/airlock": "*"
    }
}
{
    "name": "presttec\/laravel-sanctum",
    "description": "Laravel Sanctum provides a featherweight authentication system for SPAs and simple APIs.",
    "keywords": [
        "laravel",
        "sanctum",
        "auth"
    ],
    "license": "MIT",
    "support": {
        "issues": "https:\/\/github.com\/presttec\/laravel-sanctum\/issues",
        "source": "https:\/\/github.com\/presttec\/laravel-sanctum"
    },
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "presttec\/illuminate-contracts": "^6.9|^7.0|^8.0",
        "presttec\/illuminate-database": "^6.9|^7.0|^8.0",
        "presttec\/illuminate-support": "^6.9|^7.0|^8.0"
    },
    "require-dev": {
        "presttec\/mockery": "^1.0",
        "presttec\/testbench": "^4.0|^5.0|^6.0",
        "presttec\/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Sanctum\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\Sanctum\\Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/sanctum": "*"
    },
    "conflicts": {
        "laravel\/sanctum": "*"
    }
}